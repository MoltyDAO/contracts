# Environment variables for MoltyDAO
# Copy this to .env and fill in your values

# RPC Endpoints (get from Alchemy or Infura)
BASE_RPC_URL=https://mainnet.base.org
BASE_SEPOLIA_RPC_URL=https://sepolia.base.org

# Deployment wallet (testnet only for now)
# Generate a new key with: cast wallet new
PRIVATE_KEY=your_private_key_here

# Etherscan API key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# GitHub Integration (for backend)
GITHUB_TOKEN=your_github_token
GITHUB_WEBHOOK_SECRET=random_secret_for_webhooks

# Database (for VPS deployment)
DATABASE_URL=sqlite:///data/agentdao.db
